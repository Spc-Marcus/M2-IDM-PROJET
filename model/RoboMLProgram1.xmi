<?xml version="1.0" encoding="UTF-8"?>
<roboML:RoboMLProgram
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:roboML="http://www.example.org/roboML">
  <functions
      name="Entry"
      returnType="Void">
    <instructions
        xsi:type="roboML:VariableDeclaration">
      <variable
          name="count"/>
      <value
          xsi:type="roboML:Literal"
          value="0"/>
    </instructions>
    <instructions
        xsi:type="roboML:Loop">
      <condition
          xsi:type="roboML:BinaryExpression">
        <left
            xsi:type="roboML:VariableRef"
            variable="//@functions.0/@instructions.0/@variable"/>
        <right
            xsi:type="roboML:Literal"
            value="5"/>
      </condition>
      <body
          xsi:type="roboML:Assignment"
          assignee="//@functions.0/@instructions.0/@variable">
        <value
            xsi:type="roboML:BinaryExpression">
          <left
              xsi:type="roboML:VariableRef"
              variable="//@functions.0/@instructions.0/@variable"/>
          <right
              xsi:type="roboML:Literal"
              value="1"/>
        </value>
      </body>
      <body
          xsi:type="roboML:FunctionCall"
          function="//@functions.1"/>
    </instructions>
  </functions>
  <functions
      name="Square">
    <instructions
        xsi:type="roboML:Movement">
      <distance
          xsi:type="roboML:Literal"
          value="30"/>
    </instructions>
    <instructions
        xsi:type="roboML:Rotate">
      <angle
          xsi:type="roboML:Literal"
          value="90"/>
    </instructions>
    <instructions
        xsi:type="roboML:Movement"
        unit="mm">
      <distance
          xsi:type="roboML:Literal"
          value="300"/>
    </instructions>
    <instructions
        xsi:type="roboML:Rotate">
      <angle
          xsi:type="roboML:Literal"
          value="90"/>
    </instructions>
    <instructions
        xsi:type="roboML:Movement">
      <distance
          xsi:type="roboML:Literal"
          value="30"/>
    </instructions>
    <instructions
        xsi:type="roboML:Rotate">
      <angle
          xsi:type="roboML:Literal"
          value="90"/>
    </instructions>
    <instructions
        xsi:type="roboML:Movement"
        unit="mm">
      <distance
          xsi:type="roboML:Literal"
          value="300"/>
    </instructions>
    <instructions
        xsi:type="roboML:Rotate">
      <angle
          xsi:type="roboML:Literal"
          value="90"/>
    </instructions>
  </functions>
</roboML:RoboMLProgram>
